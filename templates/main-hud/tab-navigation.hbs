<div class="hud-tabs tabs"
    aria-roledescription="{{localize "SHEETS.FormNavLabel"}}">
    <div class="background-blur"></div>
    {{log tabs}}
    {{#each tabs as |tab|}}
    <a
        data-action="tab"
        data-group="primary"
        data-tab="{{tab.id}}"
        {{#if tab.cssClass}}
        class="{{tab.cssClass}}"
        {{/if}}

        {{#if tab.tooltip}}
        data-tooltip="{{tab.tooltip}}"
        {{/if}}
    >
        {{#if tab.icon}}<i class="{{tab.icon}}"></i>{{/if}}
        {{#if tab.label}}{{localize tab.label}}{{/if}}
    </a>
    {{/each}}
</div>
